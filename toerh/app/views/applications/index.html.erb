<div class="wrapper">
<p class="sign_out"><%= link_to 'Sign out', logout_path, method: :delete %></p>
<h2>Admin area</h2>
<h2>Active applications</h2>
<ul>
<% @apps.each do |app| %>
  <li>Contact: <%= app.contact_email %><br/>
    API-key: <%= app.api_key.access_token %><br/>
  <%= link_to 'Revoke', app, method: :delete, data: { confirm: 'Are you sure?' } %></li>
<% end %>
</ul>
</div>
